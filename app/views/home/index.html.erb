<%= render partial: 'home/partials/header', locals: { title: title } %>

<div class="padding--page">
  <h2 class="center"><%= theme_title %></h2>

  <div class="site-width-medium margin-center flex flex-wrap flex-h-between">
    <% themes.each do |theme| %>
