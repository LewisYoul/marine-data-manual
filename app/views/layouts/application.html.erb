<!DOCTYPE html>
<html>
  <head>
    <%= render 'global/head' %>
  </head>

  <body>

    <%= render 'global/topbar' %>

    <%= render partial: 'global/header', locals: { title: page_title } %>

    <div id="v-app">
      <%= render 'global/social-sharing' %>

      <%= yield %>
    </div>

    <%= render 'global/cta' %>

    <%= render 'global/footer' %>

    <%= javascript_pack_tag 'application' %>
    <% if Rails.env == 'production' || Rails.env == 'staging' %>
      <%= render 'shared/google_analytics' %>
    <% end %>

  </body>
</html>
